using ProtoBuf;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace PatientPortal.Domain.Models.CORE.Report
{
    [ProtoContract]
    public class RPAppointment
    {
        [ProtoMember(1)]
        public string No { get; }
        [ProtoMember(2)]
        public string PhysicianId { get; }
        [ProtoMember(3)]
        public string PhysicianName { get; }
        [ProtoMember(4)]
        public DateTime Date { get; }
        [ProtoMember(5)]
        public int Time { get; }
        [ProtoMember(6)]
        public string DepartmentName { get; }
    }
}
