/* MAIN NAV */
.mainnav-wrap{min-height:4px;padding-top: 30px;}
.main-nav .nav li {position: relative;}
.main-nav .nav li a{line-height: 40px;display: block;font-family: 'myriad_prolight', Arial,sans-serif;font-size: 18px;text-transform: uppercase;color: #151515;padding: 0 12px;margin-top: 3px;margin-right: 15px;}
.main-nav .nav li a:hover,.main-nav .nav li a.active{color: #73b744;border-top: 3px solid #1b7b57;margin-top: 0;}

.main-nav .nav ul{position: absolute;background: #fff;width: 160px;padding: 5px 15px;display: none;z-index: 99;}
.main-nav .nav li:hover>ul{display: block;}
.main-nav .nav ul ul{left: 150px;top: 0;}
.main-nav .nav ul li{width: 100%;}
.main-nav .nav ul li a{margin: 0;font-size: 14px;text-transform: none;border-top: 1px solid #e2e2e2;color: #020202;display: block;padding: 5px 0;line-height: 16px;}
.main-nav .nav ul li:first-child a , .main-nav .nav ul li:first-child a:hover {border-top: none;}
.main-nav .nav ul li a:hover{color: #3c9300;border-top: 1px solid #e2e2e2;}

.main-nav .nav .noli li:first-child a{background: none;padding-top: 0;}
/*.vi-VN .main-nav .nav ul {display: none !important;}*/

.submenu-img{float: right;}
.main-nav .noli{float: left;clear: left;text-align: left;}

.shownav{display: none;}

.main-nav li:hover>.submenu{display: block;}

