
appManagement.factory('AppointmentService', ['$resource', 'Params',
    function ($resource, Params) {
        return $resource(Params.rootUrl + 'User', {}, {
            queryDoctor: {
                method: 'GET',
                url: Params.rootUrl + 'User/Get?search=:search&type=:type',
                params: {
                    search: '@search', type: '@type'
                },
                isArray: true
            }
        });
    }]);