CREATE TABLE [dbo].[Diagnose]
(
	[MedicalRecordId] INT NOT NULL PRIMARY KEY, -- Bệnh án
    [ParaclinicalRequestId] INT NOT NULL, -- CLS yêu cầu
	[Date] SMALLDATETIME NOT NULL, -- Ngày thực hiện
    [Detail] NVARCHAR(1000) NOT NULL, -- Nội dung chi tiết
    [Result] NVARCHAR(300) NOT NULL  -- Kết quả
)
