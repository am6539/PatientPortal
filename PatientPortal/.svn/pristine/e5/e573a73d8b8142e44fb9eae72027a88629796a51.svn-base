CREATE PROCEDURE [dbo].[usp_spa_DoctorProfile]
	@UserId NVARCHAR(128)
AS BEGIN
	SET NOCOUNT ON
	SET TRANSACTION ISOLATION LEVEL READ COMMITTED

	BEGIN TRY
		IF @UserId IS NOT NULL
			SELECT * FROM [dbo].[DoctorProfile]
			WHERE [UserId] = @UserId
			ORDER BY [UserId]
		ELSE
			SELECT * FROM [dbo].[Department]
	END TRY
	BEGIN CATCH
		IF @@TRANCOUNT <> 0 
		BEGIN
			RETURN NULL
		END
	END CATCH
END