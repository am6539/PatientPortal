CREATE TABLE [dbo].[CMS]
(
	[Id] TINYINT NOT NULL PRIMARY KEY, 
	[Name] NVARCHAR(250) NOT NULL, 
	[Address] NVARCHAR(250) NOT NULL, 
	[Phone] VARCHAR(100) NOT NULL, 
	[Fax] VARCHAR(100) NOT NULL, 
	[Hotline] VARCHAR(50) NOT NULL, 
	[GoogleMap] NVARCHAR(MAX) NOT NULL, 
	[GoogleEngineScript] NVARCHAR(MAX) NOT NULL, 
	[IsSendEmailWF] BIT NOT NULL, -- Send email when changing Workflow State
	[OpeningHours] VARCHAR(50) NOT NULL, 
	[Email] VARCHAR(256) NOT NULL, 
	[Youtube] VARCHAR(256) NULL, 
	[Twitter] VARCHAR(256) NULL, 
	[Facebook] VARCHAR(256) NULL, 
	[Google] VARCHAR(256) NULL, 
	[Slogan] VARCHAR(256) NULL
)
