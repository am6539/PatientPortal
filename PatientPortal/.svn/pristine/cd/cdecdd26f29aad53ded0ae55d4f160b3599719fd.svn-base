
<!-- slider -->
<ul class="slider">
    <li ng-repeat="item in sliders" style="background-image: url('{{item.Image}}');">
        <!--<img src="" />-->
        &nbsp;
    </li>
</ul>
<div class="page relative noborder">
    <!--<div class="top_hint">
        Give us a call: +123 356 123 124
    </div>-->
